{
  "name": "Mozilla Styles",
  "description": "Adds text styles, layer styles, and fills for Mozilla products.",
  "author": "",
  "homepage": "https://github.com/mozilla/protocol-sketch-plugin",
  "identifier": "com.sketchapp.michaelmccombie.mozilla",
  "appcast": "https://raw.githubusercontent.com/mozilla/protocol-sketch-plugin/master/appcast.xml",
  "compatibleVersion": 54,
  "bundleVersion": 1,
  "icon": "icon.png",
  "commands": [
    {
      "name": "Firefox",
      "identifier": "palette-firefox",
      "script": "firefox.js",
      "handlers": {
        "run": "onRun"
      },
      "shortcut": "ctrl shift f"
    },
    {
      "name": "Mozilla",
      "identifier": "palette-mozilla",
      "script": "mozilla.js",
      "handlers": {
        "run": "onRun"
      },
      "shortcut": "ctrl shift m"
    },
    {
      "name": "Mozilla Labs",
      "identifier": "palette-mozilla-labs",
      "script": "mozillalabs.js",
      "handlers": {
        "run": "onRun"
      }
    },
    {
      "name": "Light Mode",
      "identifier": "palette-pocket-light",
      "script": "pocketlight.js",
      "handlers": {
        "run": "onRun"
      },
      "shortcut": "ctrl shift p"
    },
    {
      "name": "Dark Mode",
      "identifier": "palette-pocket-dark",
      "script": "pocketdark.js",
      "handlers": {
        "run": "onRun"
      }
    },
    {
      "name": "Sepia Mode",
      "identifier": "palette-pocket-sepia",
      "script": "pocketsepia.js",
      "handlers": {
        "run": "onRun"
      }
    },
    {
      "name": "Clear Styles",
      "identifier": "clear-styles",
      "script": "clearstyles.js",
      "handlers": {
        "run": "onRun"
      }
    }
  ],
  "menu": {
    "title": "Mozilla Styles",
    "items": [
      "palette-firefox",
      {
        "title": "Mozilla",
        "items": [
          "palette-mozilla",
          "palette-mozilla-labs"
        ]
      },
      {
        "title": "Pocket",
        "items": [
          "palette-pocket-light",
          "palette-pocket-dark",
          "palette-pocket-sepia"
        ]
      },
      "-",
      "clear-styles"
    ]
  },
  "version": "3.1.0",
  "disableCocoaScriptPreprocessor": true
}